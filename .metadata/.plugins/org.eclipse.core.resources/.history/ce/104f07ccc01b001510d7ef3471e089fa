function RifCS(record) {
	this.record = record;
}

RifCS.protorype.records = function() {
	return this.record.registryObject;
};

RifCS.protorype.record = function(i) {
	return this.records()[i];	
};

RifCS.protorype.recordKey = function(i) {
	return this.record(i).key;	
};

RifCS.protorype.recordGroup = function(i) {
	return this.record.$.group;	
};
